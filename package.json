{
  "name": "televimer-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize node ./bin/www",
    "wstart": "set DEBUG=express-base:*,app:* & nodemon ./bin/www",
    "s": "set DEBUG=express-base:*,app:* & nodemon ./bin/www",
    "devstart": "nodemon ./bin/www",
    "pm2prod": "pm2 start ecosystem.config.js --env production",
    "sync": "node ./src/config/sync.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "ali-oss": "^6.15.2",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "commitlint": "^15.0.0",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "fcm-node": "^1.6.1",
    "fs": "^0.0.1-security",
    "hat": "^0.0.3",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.2",
    "pg": "^6.1.0",
    "pug": "^3.0.2",
    "sequelize": "^6.11.0",
    "socket.io": "^4.1.2",
    "ws": "^7.5.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "cross-env": "^3.1.3",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.1",
    "expect.js": "^0.3.1",
    "lint-staged": ">=10",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@root": ".",
    "@controllers": "./src/controllers",
    "@models": "./src/models",
    "@utils": "./src/utils",
    "@commons": "./src/common",
    "@services": "./src/service",
    "@middlewares": "./src/middleware",
    "@config": "./src/config",
    "@routes": "./src/routes"
  },
  "husky": {
    "hooks": {
      "applypatch-ms": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "lint-staged": {
    "src/**/*.js": []
  }
}
